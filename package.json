{
  "name": "figact",
  "version": "1.0.0",
  "description": "A CLI tool to export Figma styles, tokens, and assets into React projects",
  "main": "dist/index.js",
  "bin": {
    "figact": "dist/bin/figact.js"
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "dev": "ts-node-dev --respawn --transpile-only src/bin/figact.ts",
    "start": "node ./dist/bin/figact.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "figma",
    "react",
    "design-tokens",
    "styles",
    "cli",
    "figact"
  ],
  "author": "Vivek Sharma",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^24.10.2",
    "eslint": "^9.39.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
